@import '../../tpls/timetables/timetables.wxss';

.picker {
  background-color: #066fa5;
  width: 100%;
  height: 80rpx;
  color: #f4f4f4;
  text-align: center;
  font-size: 40rpx;
  line-height: 80rpx;
  position: fixed;
  top: 0;
}

.picker-xq, .picker-week {
  float: left;
  width: 50%;
}

.week-tab {
  margin-top: 80rpx;
}

.tab-panel {
  min-height: 0;
}